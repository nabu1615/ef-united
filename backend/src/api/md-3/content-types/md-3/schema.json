    "singularName": "md-3",
    "pluralName": "md-3s",
    "displayName": "MD3",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "evidence": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match"
    },
    "state": {
      "type": "enumeration",
      "enum": [
        "approved",
        "pending",
        "rejected"
      ],
      "default": "pending"
    }
  }
